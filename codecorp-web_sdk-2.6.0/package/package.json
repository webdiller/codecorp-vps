{
  "name": "codecorp-web_sdk",
  "version": "2.6.0",
  "description": "Code By Brady Cortex Decoder Web SDK package compatible with both Web and Node",
  "files": [
    "dist/node",
    "dist/web",
    "dist/enums",
    "Proprietary Rights Notice.txt",
    "README.md"
  ],
  "types": "./dist/web/CortexDecoderWeb.d.ts",
  "exports": {
    ".": {
      "require": "./dist/node/CortexDecoderNode.js",
      "import": "./dist/web/CortexDecoderWeb.js",
      "fallback": "./dist/web/CortexDecoderWeb.js"
    }
  },
  "scripts": {
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "build-package": "node build-package.js",
    "test-web-features": "karma start --single-run --browsers ChromeHeadless,FirefoxHeadless features.karma.conf.js",
    "test-node-features": "mocha test/node-features-test.js",
    "test-license-CL": "karma start --single-run --license_type=CL --browsers ChromeHeadless,FirefoxHeadless licenses.karma.conf.js",
    "test-license-EL1": "karma start --single-run --license_type=EL1 --browsers ChromeHeadless,FirefoxHeadless licenses.karma.conf.js",
    "test-license-EL2": "karma start --single-run --license_type=EL2 --browsers ChromeHeadless,FirefoxHeadless licenses.karma.conf.js",
    "test-license-ALL": "karma start --single-run --license_type=ALL --browsers ChromeHeadless,FirefoxHeadless licenses.karma.conf.js",
    "test-node-license-CL": "mocha --license_type=CL test/node-license-test.js",
    "test-node-license-EL1": "mocha --license_type=EL1 test/node-license-test.js",
    "test-node-license-EL2": "mocha --license_type=EL2 test/node-license-test.js",
    "test-node-license-ALL": "mocha --license_type=ALL test/node-license-test.js",
    "test-qr-license-CL": "karma start --single-run --license_type=CL --browsers ChromeHeadless,FirefoxHeadless qr-config.karma.conf.js",
    "test-qr-license-EL1": "karma start --single-run --license_type=EL1 --browsers ChromeHeadless,FirefoxHeadless qr-config.karma.conf.js",
    "test-qr-license-EL2": "karma start --single-run --license_type=EL2 --browsers ChromeHeadless,FirefoxHeadless qr-config.karma.conf.js",
    "test-qr-license-ALL": "karma start --single-run --license_type=ALL --browsers ChromeHeadless,FirefoxHeadless qr-config.karma.conf.js",
    "test-symbologies-CL": "karma start --single-run --license_type=CL --browsers ChromeHeadless,FirefoxHeadless symbologies.karma.conf.js",
    "test-symbologies-EL1": "karma start --single-run --license_type=EL1 --browsers ChromeHeadless,FirefoxHeadless symbologies.karma.conf.js",
    "test-symbologies-EL2": "karma start --single-run --license_type=EL2 --browsers ChromeHeadless,FirefoxHeadless symbologies.karma.conf.js",
    "test-symbologies-ALL": "karma start --single-run --license_type=ALL --browsers ChromeHeadless,FirefoxHeadless symbologies.karma.conf.js",
    "test-node-symbologies-CL": "mocha --license_type=CL test/node-symbology-test.js",
    "test-node-symbologies-EL1": "mocha --license_type=EL1 test/node-symbology-test.js",
    "test-node-symbologies-EL2": "mocha --license_type=EL2 test/node-symbology-test.js",
    "test-node-symbologies-ALL": "mocha --license_type=ALL test/node-symbology-test.js",
    "test-node": "npm run test-node-features && npm run test-node-symbologies-CL && npm run test-node-symbologies-EL1 && npm run test-node-symbologies-EL2 && npm run test-node-symbologies-ALL",
    "test-web": "npm run test-web-features && npm run test-symbologies-CL && npm run test-symbologies-EL1 && npm run test-symbologies-EL2 && npm run test-symbologies-ALL",
    "clean": "shx rm -rf dist",
    "docs": "npx typedoc --out docs src/index.ts"
  },
  "keywords": [],
  "author": "Skanda Kumar Srikanta",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@open-wc/karma-esm": "^4.0.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.249",
    "@types/clientjs": "^0.2.0",
    "@types/emscripten": "^1.39.6",
    "@types/graceful-fs": "^4.1.6",
    "@types/jest": "^26.0.22",
    "@types/node": "^20.2.3",
    "@types/w3c-image-capture": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^9.1.2",
    "browserify": "^17.0.0",
    "buffer": "^6.0.3",
    "chai": "^4.3.7",
    "clean-webpack-plugin": "^4.0.0",
    "clientjs": "^0.2.1",
    "crypto-browserify": "^3.12.0",
    "cypress": "^10.7.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "exports-loader": "^4.0.0",
    "file-loader": "^6.2.0",
    "fs": "^0.0.2",
    "jimp": "^0.22.10",
    "karma": "^6.4.1",
    "karma-browserify": "^8.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-firefox-launcher": "^2.1.2",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-ie-launcher": "^1.0.0",
    "karma-log-reporter": "^0.0.4",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^10.2.0",
    "shx": "^0.3.4",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^8.1.0",
    "typedoc": "^0.23.26",
    "typescript": "^5.0.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-node-externals": "^3.0.0",
    "worker-loader": "^3.0.8"
  },
  "dependencies": {
    "@types/dom-mediacapture-transform": "^0.1.9",
    "iconv-lite": "^0.6.3",
    "image-capture": "^0.4.0"
  }
}
